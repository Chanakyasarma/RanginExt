{
    "name":"RanginExt",
    "description":"A color picker extension",
    "version":"0.1",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "48":"/images/color-picker.png"
    }
}